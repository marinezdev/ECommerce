    <style>
        .wrap-menu-desktop {
            position: fixed;
            z-index: 101;
            /* background-color: #e9ecefc2; */
            background-color: #ffffffe0;
            width: 100%;
            height: 84px;
            top: 0px;
            left: 0px;
            -webkit-transition: height 0.3s, background-color 0.3s;
            -o-transition: height 0.3s, background-color 0.3s;
            -moz-transition: height 0.3s, background-color 0.3s;
            transition: height 0.3s, background-color 0.3s;
        }

        .how-itemcart1 {
            width: 130px;
            position: relative;
            margin-right: 20px;
            cursor: pointer;
        }

        .bor14 {
            border-radius: 10px;
        }

        .how-itemcart1::after {
            content: '';
            font-family: Linearicons-Free;
            font-size: 16px;
            display: -webkit-box;
            display: -webkit-flex;
            display: -moz-box;
            display: -ms-flexbox;
            display: flex;
            justify-content: center;
            align-items: center;
            position: absolute;
            width: 100%;
            height: 100%;
            top: 0;
            left: 0;
            background-color: rgb(0 0 0 / 0%);
            color: #fff;
            transition: all 0.3s;
            -webkit-transition: all 0.3s;
            -o-transition: all 0.3s;
            -moz-transition: all 0.3s;
            opacity: 0;
        }

        .BtnOXXO {
            font-family: "Proxima Nova",-apple-system,"Helvetica Neue",Helvetica,Roboto,Arial,sans-serif;
            border-color: #ffc62c;
            background-color: #eb0628;
            border: 1px solid #ffc62c;
            color: #fff;
            border-radius: 10px;
            width: 100%;
            height: 50px;
            font-weight: bold;
        }

            .BtnOXXO:hover {
                background-color: #ffc62c;
                transition: all 0.4s;
            }


        /****************************************************************************************************************/
        .opps {
            width: 496px;
            border-radius: 4px;
            box-sizing: border-box;
            padding: 0 45px;
            margin: 20px auto;
            overflow: hidden;
            border: 1px solid #b0afb5;
            font-family: 'Open Sans', sans-serif;
            color: #4f5365;
        }

        .opps-reminder {
            position: relative;
            top: -1px;
            padding: 9px 0 10px;
            font-size: 11px;
            text-transform: uppercase;
            text-align: center;
            color: #ffffff;
            background: #000000;
        }

        .opps-info {
            margin-top: 26px;
            position: relative;
        }

            .opps-info:after {
                visibility: hidden;
                display: block;
                font-size: 0;
                content: " ";
                clear: both;
                height: 0;
            }

        .opps-brand {
            width: 45%;
            float: left;
        }

            .opps-brand img {
                max-width: 150px;
                margin-top: 2px;
            }

        .opps-ammount {
            width: 55%;
            float: right;
        }

            .opps-ammount h2 {
                font-size: 36px;
                color: #000000;
                line-height: 24px;
                margin-bottom: 15px;
            }

                .opps-ammount h2 sup {
                    font-size: 16px;
                    position: relative;
                    top: -2px
                }

            .opps-ammount p {
                font-size: 10px;
                line-height: 14px;
            }

        .opps-reference {
            margin-top: 14px;
        }

        .opps-footnote {
            margin-top: 22px;
            padding: 22px 20 24px;
            color: #108f30;
            text-align: center;
            border: 1px solid #108f30;
            border-radius: 4px;
            background: #ffffff;
        }

        .opps-instructions {
            margin: 32px -45px 0;
            padding: 32px 45px 45px;
            border-top: 1px solid #b0afb5;
            background: #f8f9fa;
        }

        .opps h1 {
            font-size: 27px;
            color: #000000;
            text-align: center;
            margin-top: -1px;
            padding: 6px 0 7px;
            border: 1px solid #b0afb5;
            border-radius: 4px;
            background: #f8f9fa;
        }

        .opps h3 {
            margin-bottom: 10px;
            font-size: 15px;
            font-weight: 600;
            text-transform: uppercase;
        }

        ol {
            margin: 17px 0 0 16px;
        }

        li + li {
            margin-top: 10px;
            color: #000000;
        }

        a {
            color: #1155cc;
        }

        .bg1 {
            background-color: #b0ce68;
        }
    </style>

</head>
<body>
    <!-- Header -->
    <header>
        <!-- Header desktop -->
        <div class="container-menu-desktop">

            <div class="wrap-menu-desktop">
                <nav class="limiter-menu-desktop container">

                    <!-- Logo desktop -->
                    <a href="#" class="logo">
                        <img src="~/images/icons/logoCSB.png" />
                    </a>

                    <!-- Menu desktop -->
                    <div class="menu-desktop">
                        <ul class="main-menu">
                        </ul>
                    </div>

                    <!-- Icon header -->
                    <div class="wrap-icon-header flex-w flex-r-m">
                        <a href="@Url.Action("Index", "Store")" class="flex-c-m cl0 size-103 bg1 bor1 hov-btn2 p-lr-15 trans-04">
                            <i class="fa fa-reply" aria-hidden="true"></i>  &nbsp;&nbsp;  Volver a Tienda
                        </a>
                    </div>
                </nav>
            </div>
        </div>
    </header>



    <div class="top-content">
        <br /><br /><br /><br />
        <div class=" bor15 p-t-18 p-b-15 p-lr-40 p-lr-15-sm" style="border-top: 1px solid #e6e6e6; border-radius: 5px; background: linear-gradient(to right, #185a9d, #4927b0); color: #e6e6e6 ">
            <div class="row">
                <div class="col-sm-12 col-lg-12 m-lr-auto m-b-50 text-center">
                    <h1 style="font-size: 22px; padding-bottom: 20px;"><strong>Listo, solo falta tu pago</strong> </h1>
                    <h1 style="font-size: 22px; padding-bottom: 20px;">Numero de pedido: <strong>@ViewBag.Folio</strong> </h1>
                    <h1 style="font-size: 14px; padding-bottom: 20px;"><strong>Solo falta tu pago para completar tu pedido</strong>. Ve al Oxxo más cercano o que prefieras y <br /><strong> paga dentro de los próximos 6 días hábiles a partir de tu compra </strong>. </h1>
                    <h1 style="font-size:14px">Una vez que realices tu depósito <strong>recibirás un correo electrónico con la información de tu compra</strong> <br />en un plazo máximo de <strong>48 horas hábiles</strong>.</h1>
                </div>
                <div class="col-lg-5 text-center">
                </div>
                <div class="col-sm-2 col-lg-2 text-center">
                    <a href="/Store" class="flex-c-m cl0 size-103 bg1 bor1 hov-btn2 p-lr-15 trans-04" onclick="javascript: window.print();">
                        <i class="fa fa-print" aria-hidden="true"></i>  &nbsp;&nbsp;  Imprimir
                    </a>
                </div>
                <div class="col-lg-5 text-center">
                </div>
            </div>
        </div>
        <br />
        <div class="container">
            <div class="row">
                <div class="bor15 col-sm-5" style="border-top: 1px solid #e6e6e6; border-radius: 5px; background: linear-gradient(to right, #f7f7f7, #f5f5f5); ">
                    <div class="row">
                        <div class="col-sm-12 col-lg-12 m-lr-auto m-b-50">
                            <br />
                            <h1 style="font-size: 22px; color: #403f3f;" class="text-center"><strong> Resumen de compra </strong>  </h1>
                            <br />

                            <div class="flex-w  bor12 p-b-13" style="background-color: #e9ecef; padding: 20px;">

                                <div class="size-208">
                                    <h1 style="font-size: 18px; padding-bottom: 20px; color: #403f3f;">Subtotal: </h1>
                                </div>

                                <div class="size-209">
                                    <span class="mtext-110 cl2 text-right" id="SubTotal_">
                                        $@ViewBag.Total
                                    </span>
                                </div>
                            </div>

                            <div class="flex-w  bor12 p-b-13" style="background-color: #ffffff; padding: 20px;">

                                <div class="size-208">
                                    <h1 style="font-size: 18px; padding-bottom: 20px; color: #403f3f;">Envio: </h1>
                                </div>

                                <div class="size-209 p-r-18 p-r-0-sm w-full-ssm">
                                    <p class="mtext-110 cl2" id="TotalEnvio_">
                                        $0.00
                                    </p>
                                </div>
                            </div>

                            <div class="flex-w p-t-27 p-b-33" style="background-color: #e9ecef; padding: 20px;">
                                <div class="size-208">
                                    <h1 style="font-size: 18px; padding-bottom: 20px; color: #403f3f;">Total: </h1>
                                </div>

                                <div class="size-209 p-t-1">
                                    <span class="mtext-110 cl2" id="Total_">
                                        $@ViewBag.Total
                                    </span>
                                </div>
                            </div>

                            <br />
                            <h5 class="mtext-109 cl2 p-b-5 text-center">Entrega a domicilio</h5>
                            <div class="flex-w bor12 p-t-15 p-b-30">
                                <div class=" p-r-18 p-r-0-sm w-full-ssm">
                                    <strong><label style="font-size: 16px; color: #222222;"><i class="fa fa-address-book"></i> Información del cliente</label></strong>
                                    <p class="stext-111  p-t-2"><strong>Nombre: </strong> @ViewBag.Usuarios_Direcciones.Nombre @ViewBag.Usuarios_Direcciones.ApellidoPaterno @ViewBag.Usuarios_Direcciones.ApellidoMaterno</p>
                                    <p class="stext-111  p-t-2"><strong>Correo electrónico: </strong> @ViewBag.Usuarios_Direcciones.Correo </p>
                                    <p class="stext-111  p-t-2"><strong>Telefono: </strong> @ViewBag.Usuarios_Direcciones.Telefono - @ViewBag.Usuarios_Direcciones.TipoTelefono</p>
                                    <hr />
                                    <strong><label style="font-size: 16px; color: #222222;"><i class="fa fa-map-marker"></i> Dirección </label></strong>
                                    <p class="stext-111  p-t-2">
                                        @ViewBag.Usuarios_Direcciones.Estado, @ViewBag.Usuarios_Direcciones.Poblacion, @ViewBag.Usuarios_Direcciones.Colonia,
                                        @ViewBag.Usuarios_Direcciones.CP, @ViewBag.Usuarios_Direcciones.Calle, @ViewBag.Usuarios_Direcciones.NumExterior,
                                        @if (ViewBag.Usuarios_Direcciones.NumInteriror.Length > 0)
                                        {
                                            @ViewBag.Usuarios_Direcciones.NumInteriror <span>, </span>
                                        }
                                        @if (ViewBag.Usuarios_Direcciones.EntreCalles.Length > 0)
                                        {
                                            @ViewBag.Usuarios_Direcciones.EntreCalles <span>, </span>
                                        }
                                        @if (ViewBag.Usuarios_Direcciones.Referencias.Length > 0)
                                        {
                                            @ViewBag.Usuarios_Direcciones.Referencias <span>, </span>
                                        }
                                    </p>
                                    @if (ViewBag.Usuarios_Direcciones.RecibirPedido > 0)
                                    {
                                        <p class="stext-111  p-t-2"><strong>Persona quien recibirá el pedido: </strong> </p>
                                        <p class="stext-111  p-t-2"><strong>Nombre: </strong> @ViewBag.Usuarios_Direcciones.Recibe_Nombre @ViewBag.Usuarios_Direcciones.Recibe_Apellidos </p>
                                        <p class="stext-111  p-t-2"><strong>Telefono: </strong> @ViewBag.Usuarios_Direcciones.Recibe_Telefono - @ViewBag.Usuarios_Direcciones.Recibe_TipoTelefono</p>
                                    }

                                </div>
                            </div>
                        </div>

                    </div>
                </div>

                <div class=" col-sm-1">
                </div>

                <div class=" bor15 col-sm-6" style="border-top: 1px solid #e6e6e6; border-radius: 5px; ">
                    <div class="row">
                        <div class="col-sm-12 col-lg-12">
                            <br />
                            <h1 style="font-size: 22px; color: #403f3f;" class="text-center"><strong> ! COMPRA FINALIZADA  ! </strong>  </h1>

                            <div class="bor10 p-lr-40 p-t-30 p-b-40 m-l-63 m-r-40 m-lr-0-xl p-lr-15-sm text-center">
                                <p style="font-size:20px"> Folio de seguimiento: <strong>  @ViewBag.Folio</strong> </p>

                                <br />
                                <a href="@Url.Action("Index", "Store")" class="flex-c-m stext-101 cl0 size-116 bg3 bor14 hov-btn3 p-lr-15 trans-04 pointer">
                                    Volver a Tienda
                                </a>
                                <br />
                                <a href="@Url.Action("Index", "RastreaPedidos")" class="flex-c-m stext-101 cl0 size-116 bg1 bor14 hov-btn3 p-lr-15 trans-04 pointer">
                                    Rastrear pedido
                                </a>
                                <br />
                                <!-- Set up a container element for the button -->
                                @*<p style="font-size:20px">Los detalles de tu compra aparecerán una vez que se procese el pago</p>*@
                                <br />
                                <h5><strong>( Para aclaraciones: email@contacto.com )</strong></h5>
                                <br />
                                <h5><strong>Teléfonos: 55@@@@@@@@ - 55@@@@@@@@</strong></h5>


                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <br />
            <div class="row">
                <div class=" bor15 col-sm-12" style="border-top: 1px solid #e6e6e6; border-radius: 5px; ">
                    <p class="stext-111  p-t-2">
                        *La fecha de entrega puede cambiar por la disponibilidad del producto, tu ubicación o la forma de pago.
                    </p>
                    <br />
                    <table>
                        @{

                            foreach (var Arti in ViewBag.ListaArticulos)
                            {
                                <tr>
                                    <td>
                                        <div class="how-itemcart1">
                                            <img src="https://@Arti.Imagen" alt="IMG">
                                        </div>
                                    </td>
                                    <td>
                                        <p>
                                            <strong>@Arti.Nombre</strong>
                                        </p>
                                        <label style="margin-bottom: 0rem; font-size: 12px; ">Categoria: <strong style="text-transform: lowercase; ">@Arti.Clasificacion</strong></label>
                                        Cantidad : <strong>@Arti.Item</strong>
                                        <strong><label id="Total_@Arti.Id">Total: @Arti.TotalText</label></strong>
                                        <br />
                                    </td>
                                </tr>
                            }

                        }
                    </table>

                </div>
            </div>
        </div>
        <br />
    </div>

    <div class="bg6 flex-c-m flex-w" style="background: linear-gradient(to right, #185a9d, #9C27B0);">
        <span class="stext-107 cl6 p-lr-25">
            <label id="m_TipoCategoriaS"></label>
        </span>
        <span class="stext-107 cl6 p-lr-25">
        </span>
