                                }
                                <hr />
                                <p class="stext-111  p-t-2">
                                    *La fecha de entrega puede cambiar por la disponibilidad del producto, tu ubicación o la forma de pago.
                                </p>
                            </div>
                        </div>

                        <br />
                        <div class="flex-w bor12 p-t-15 p-b-30">
                            <div class=" p-r-18 p-r-0-sm w-full-ssm">
                                <table>
                                    @{

                                        foreach (var Arti in ViewBag.ListaArticulos)
                                        {
                                            <tr>
                                                <td>
                                                    <div class="how-itemcart1">
                                                        <img src="https://@Arti.Imagen" alt="IMG">
                                                    </div>
                                                </td>
                                                <td>
                                                    <p>
                                                        <strong>@Arti.Nombre</strong>
                                                    </p>
                                                    <label style="margin-bottom: 0rem; font-size: 12px; text-transform: lowercase; ">Categoria: <strong style="">@Arti.Clasificacion</strong></label>
                                                    Cantidad : <strong>@Arti.Item</strong>
                                                    <strong><label id="Total_@Arti.Id">Total: @Arti.TotalText</label></strong>
                                                    <br />
                                                </td>
                                            </tr>
                                        }

                                    }
                                </table>
                            </div>
                        </div>
                        <br />
                        <div class="flex-w text-right" style="font-size:25px">
                            <div>
                                <strong>
                                    <span class="">
                                        Total:
                                    </span>
                                    <span class="" id="Total_">
                                        $ @ViewBag.Total
                                    </span>
                                </strong>
                            </div>
                        </div>
                        <br />
                        <a href="@Url.Action("Paypal", "MetodoPago")" class="flex-c-m stext-101 cl0 size-116 bg3 bor14 hov-btn3 p-lr-15 trans-04 pointer" id="Btn_Pagar2" style="display: none;">
