                            <div class="row">
                                <div class="col-md-12">
                                    <h5><strong style=" color: #222222;">1.- FORMA DE PAGO </strong> </h5>
                                    <hr />
                                    <div class="form-group">
                                        <label for="exampleFormControlSelect1"><strong>* Método de pago: </strong></label>
                                        <select class="form-control" id="MetodoPago" onchange="callMetodoPago(this.value)">
                                            <option value="0">Elige el método de pago </option>
                                            <option value="1">PayPal </option>
                                            <option value="2">OXXO Pay</option>
                                            <option value="3">SPEI</option>
                                            <option value="4">Tarjeta de crédito y débito </option>
                                        </select>
                                    </div>
                                </div>
                            </div>
