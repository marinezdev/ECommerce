    <script>
        function Buscar()
        {
            if (Validar()) {
                var Folio = $('#Folio').val();
                window.location.href = '@Url.Action("Rastreo", "RastreaPedidos")?F=' + Folio;
            } else {
                $.notify({
                    icon: 'fa fa-paw',
                    message: "Debes completar el formulario de información.",
                }, {
                    type: 'warning',
                    allow_dismiss: false,
                    animate: {
                        enter: 'animated rollIn',
                        exit: 'animated rollOut'
                    }
                });
            }
        }

        function Validar() {
            var result = false;

            $('#Folio').css("border", "1px solid #ff0049");

            if ($('#Folio').val().length > 0) {
                $('#Folio').css("border", "1px solid #cccccc");
            }

            if ($('#Folio').val().length > 0) {
                result = true;
            }

            return result;
        }
    </script>
